<h1>Price List</h1>

<%= form_tag products_path, :method => 'get', :id => "products_search" do %>
  <p>
    <%= search_field_tag :search, params[:search] %>
    <%= image_submit_tag("lgo.png", :class => "go_button") %>
  </p>
  <div id="products"><%= render 'products' %></div>
<% end %>

