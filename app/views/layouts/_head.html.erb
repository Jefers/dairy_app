<meta charset="utf-8">
<%-# Always force latest IE rendering engine. Remove this if you use the .htaccess fix %>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<%-# Use content_for :description in views to set page description %>
<meta name='description' content="<%= content_for?(:description) ? yield(:description) : 'Walden Dairy Saffron Walden Milkman Milk Delivery and more.' %>">
<meta name="author" content="Walden Dairy">
<title><%= title %></title>
<link rel="icon" href="/images/favicon.png" type="image/x-icon" />
<link rel="icon" href="/images/favicon.gif" type="image/x-icon" />
<%= render 'layouts/javascripts' %>
<%= render 'layouts/stylesheets' %>
<%= csrf_meta_tag %>